// Top 20 genes by betweenness in diseaseGene projection
CALL gds.betweenness.stream('diseaseGene')
YIELD nodeId, score
WITH nodeId, score
WHERE gds.util.asNode(nodeId):Gene
RETURN gds.util.asNode(nodeId).entrez_id AS entrez_id,
       gds.util.asNode(nodeId).symbol    AS symbol,
       score AS betweenness
ORDER BY betweenness DESC
LIMIT 20;
