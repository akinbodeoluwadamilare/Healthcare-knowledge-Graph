CALL () {
    LOAD CSV WITH HEADERS FROM 'file:///nodes_compound.csv' AS row
    WITH row
    WHERE row.drugbank_id IS NOT NULL AND row.drugbank_id <> ''
    MATCH (c:Compound { drugbank_id: row.drugbank_id })
    MATCH (d:Drug     { drugbank_id: row.drugbank_id })
    MERGE (c)-[:SAME_AS]->(d)
} IN TRANSACTIONS OF 1000 ROWS;
